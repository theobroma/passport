{
  "extends": "airbnb-base",
  "env": {
    "node": true
  },
  "plugins": ["import"],
  "rules": {
    "comma-dangle": [
      "error",
      {
        "arrays": "never",
        "objects": "never",
        "imports": "never",
        "exports": "never",
        "functions": "ignore"
      }
    ],
    "eqeqeq": 0,
    "no-unused-vars": 0,
    "no-console": 0,
    "no-shadow": 0,
    "no-plusplus": 0,
    "func-names":0,
    "global-require":0,
    "no-use-before-define":0,
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
  }
}
